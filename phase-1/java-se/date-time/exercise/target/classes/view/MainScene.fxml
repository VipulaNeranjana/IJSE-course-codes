<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep10.app.controller.MainSceneController">
   <children>
      <ComboBox fx:id="cmbType" layoutX="33.0" layoutY="26.0" onAction="#cmbTypeOnAction" prefHeight="36.0" prefWidth="541.0" promptText="Select the type" />
