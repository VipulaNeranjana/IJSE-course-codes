      <DatePicker fx:id="dtpOut" layoutX="30.0" layoutY="159.0" onAction="#dtpOutOnAction" prefHeight="30.0" prefWidth="541.0" />
      <Label fx:id="lblHint" layoutX="179.0" layoutY="207.0" text="Select in and Out Days">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <Label fx:id="lblTotal" layoutX="244.0" layoutY="294.0" text="Total" textFill="#153be4">
