      <ComboBox fx:id="cmbSomething" layoutX="25.0" layoutY="32.0" prefHeight="24.0" prefWidth="550.0" AnchorPane.leftAnchor="25.0" AnchorPane.rightAnchor="25.0" AnchorPane.topAnchor="75.0" />
      <HBox layoutX="25.0" layoutY="23.0" prefHeight="38.0" prefWidth="550.0" spacing="10.0" AnchorPane.leftAnchor="25.0" AnchorPane.rightAnchor="25.0">
         <children>
            <TextField fx:id="txtInput" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
            <Button fx:id="btnAdd" mnemonicParsing="false" onAction="#btnAddOnAction" prefWidth="60.0" text="add" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <Label fx:id="lblOutput" alignment="CENTER" layoutX="23.0" layoutY="140.0" prefHeight="32.0" prefWidth="550.0" text="Select Something to Display" AnchorPane.leftAnchor="23.0" AnchorPane.rightAnchor="27.0" />
   </children>
